{
	"name": "status-ref",
	"version": "0.0.3",
	"description": "A boolean status controller, can track and trigger status changes",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		},
		"./vue": {
			"require": "./dist/vue/index.cjs",
			"import": "./dist/vue/index.mjs",
			"types": "./dist/vue/index.d.ts"
		},
		"./react": {
			"require": "./dist/react/index.cjs",
			"import": "./dist/react/index.mjs",
			"types": "./dist/react/index.d.ts"
		}
	},
	"sideEffects": false,
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rollup -c",
		"test": "vitest"
	},
	"keywords": [
		"vue",
		"react",
		"state",
		"status",
		"boolean",
		"JavaScript",
		"TypeScript"
	],
	"author": "Yige Wang <wyg1776686515@gmail.com>",
	"homepage": "https://github.com/wangyige0701/status-ref",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wangyige0701/status-ref.git"
	},
	"license": "MIT",
	"dependencies": {
		"@wang-yige/utils": "^0.0.5"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.1.1",
		"@rollup/plugin-commonjs": "^28.0.0",
		"@rollup/plugin-node-resolve": "^15.3.0",
		"@testing-library/react": "^16.0.1",
		"@types/react": "^18.3.8",
		"@vue/reactivity": "^3.5.8",
		"jsdom": "^25.0.1",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"rollup": "^4.22.5",
		"rollup-plugin-delete": "^2.1.0",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-typescript2": "^0.36.0",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vitest": "^2.1.1"
	},
	"packageManager": "pnpm@9.1.0"
}
